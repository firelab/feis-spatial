<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
		<resource-bundle>
			<base-name>com.FEIS.messages</base-name>
			<var>msgs</var>
		</resource-bundle>

	</application>
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>searchByOther</from-outcome>
			<to-view-id>/SearchByOther.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>searchByFireStudy</from-outcome>
			<to-view-id>/FireStudySearch.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>reviewResults</from-outcome>
			<to-view-id>/ReviewResults.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/SearchByOther.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>reviewResults</from-outcome>
			<to-view-id>/ReviewResults.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/ReviewResults.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>searchByOther</from-outcome>
			<to-view-id>/SearchByOther.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/ReviewResults.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>searchByFireStudy</from-outcome>
			<to-view-id>/FireStudySearch.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/ReviewResults.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>searchByName</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/FireStudySearch.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>fireStudyResults</from-outcome>
			<to-view-id>/FireStudyResults.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>fireStudyResults</from-outcome>
			<to-view-id>/FireStudyResults.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/ReviewResults.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>fireStudyResults</from-outcome>
			<to-view-id>/FireStudyResults.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/FireStudyResults.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>reviewResults</from-outcome>
			<to-view-id>/ReviewResults.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/FireStudyResults.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>searchByFireStudy</from-outcome>
			<to-view-id>/FireStudySearch.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>navBack</display-name>
		<from-view-id>/FireStudyResults.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>navTo</from-outcome>
			<to-view-id>/FireStudySearch.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>


</faces-config>
